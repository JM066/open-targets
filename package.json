{
	"name": "project",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"heroku-postbuild": "npm run build",
		"start": "npx serve dist -s",
		"lint": "eslint .",
		"preview": "vite preview",
		"test": "vitest",
