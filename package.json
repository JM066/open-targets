		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.0",
		"eslint": "^9.33.0",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.39.1",
		"vite": "^7.1.2",
		"vitest": "^3.2.4"
